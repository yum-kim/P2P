#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules/webpack/bin/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules/webpack/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules/webpack/bin/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules/webpack/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/webpack@4.47.0/node_modules:/Users/yumi/study/P2P/front/apps/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
